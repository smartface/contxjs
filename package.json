{
  "name": "@smartface/contx",
  "version": "1.0.0-rc.1",
  "description": "Calendar Component",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --debug ./test/**/*.test.js",
    "test:watch": "mocha --watch --compilers js:babel-core/register --debug ./test/**/*.test.js",
    "build": "babel ./src/ --out-dir -d ./lib/.",
    "build:dev": "babel ./src/ --out-dir -d ../scripts/node_modules/@smartface/contx",
    "dev:sync": "mkdir -p ../scripts/node_modules/@smartface/contx && cp -r -t ../scripts/node_modules/@smartface/contx lib package.json ",
    "dev:link": "npm run build && npm run dev:sync",
    "postinstall": "npm run build",
    "docs:create": "jsdox ./src -o docs",
    "jase": "jase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartface/sf-component-calendar.git"
  },
  "keywords": [
    "javascript",
    "mobile",
    "styling",
    "style"
  ],
  "author": "Cenk Cetinkaya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartface/contxjs/issues"
  },
  "homepage": "https://github.com/smartface/contxjs#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.1",
    "cross-env": "^4.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "cz-customizable": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "jase": "^1.2.0",
    "jsdox": "^0.4.10",
    "minimist": "^1.2.0",
    "mocha": "^3.3.0",
    "semver": "^5.3.0"
  },
  "dependencies": {
    "@smartface/styler": "^1.3.14",
    "filtrex": "^0.5.4",
    "install": "^0.10.1",
    "moment": "^2.18.1",
    "moment-hijri": "^2.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "path": "./node_modules/cz-conventional-changelog",
      "config": "./.cz-config.js"
    }
  }
}
